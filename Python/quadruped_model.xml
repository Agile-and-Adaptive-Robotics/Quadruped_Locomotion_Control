    <muscle name="L_hip_joint_ext_muscle" tendon="hip_L_ext_tendon" class="muscle"/>
    <muscle name="L_hip_joint_flx_muscle" tendon="hip_L_flx_tendon" class="muscle"/>

    <muscle name="L_knee_joint_ext_muscle" tendon="knee_L_ext_tendon" class="muscle"/>
