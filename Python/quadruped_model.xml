
<mujoco model="leg">
  <option timestep="0.005"/>

  <visual>
    <map force="0.1" zfar="30"/>
    <rgba haze="0.15 0.25 0.35 1"/>
    <global offwidth="2560" offheight="1440" elevation="-20" azimuth="120"/>
  </visual>

  <asset>
    <texture type="skybox" builtin="gradient" rgb1=".3 .5 .7" rgb2="0 0 0" width="32" height="512"/>
    <texture name="body" type="cube" builtin="flat" mark="cross" width="128" height="128" rgb1="0.9 0.9 0.9" rgb2="0.9 0.9 0.9" markrgb="1 1 1"/>
    <material name="body" texture="body" texuniform="true" rgba="0.9 0.9 0.9 .5"/>
    <texture name="grid" type="2d" builtin="checker" width="512" height="512" rgb1=".1 .2 .3" rgb2=".2 .3 .4"/>
    <material name="grid" texture="grid" texrepeat="1 1" texuniform="true" reflectance=".2"/>
  </asset>

  <default>
    <motor ctrlrange="-1 1" ctrllimited="true"/>
    <default class="body">

      <!-- geoms -->
      <geom type="box" condim="1" friction=".7" solimp=".9 .99 .003" solref=".015 1" material="body" group="1"/>
      <default class="support">
        <geom fromto="-1 0 0 2 0 0"  size=".02"/>
      </default>
      <default class="pulley_wheel_left">
        <geom type="cylinder" fromto="0 -0.05500000000000001 0 0 0 0" size="0.02"/>
      </default>
      <default class="pulley_wheel_right">
        <geom type="cylinder" fromto="0 0 0 0 0.05500000000000001 0" size="0.02"/>
      </default>
      <default class="spine">
        <geom fromto="0 0 0.06 -0.4925 0 0"  size=".015"/>
      </default>
      <default class="scapula">
        <geom fromto="0 0 0 0 0 -0.17"  size=".01"/>
      </default>
      <default class="humerus">
        <geom fromto="0 0 0 0 0 -0.1925"  size=".01"/>
      </default>
      <default class="radius">
        <geom fromto="0 0 0 0 0 -0.2125"  size=".01"/>
      </default>
      <default class="hand">
        <geom fromto="0 0 0 0 0 -0.13" size=".01"/>
      </default>
      <default class="femur">
        <geom fromto="0 0 0 0 0 -0.2225" size=".01"/>
      </default>
      <default class="tibia">
        <geom fromto="0 0 0 0 0 -0.22" size=".01"/>
      </default>
      <default class="foot">
        <geom fromto="0 0 0 0 0 -0.16" size=".01"/>
      </default>

      <!-- joints -->
      <joint type="hinge" damping=".01" stiffness="0" armature=".01" limited="true" solimplimit="0 .99 .01"/>
      <default class="support_joint_horizontal">
        <joint type="slide" range="-1 30" pos="0 0 0" axis="1 0 0" stiffness="0.2"/>
      </default>
      <default class="support_joint_vertical">
        <joint type="slide" range="0.1 2" pos="0 0 0" axis="0 0 1" stiffness="0.2"/>
      </default>
      <default class="support_joint_sagittal_rotation">
        <joint type="hinge" range="-8 -5" pos="0 0 0" axis="0 1 0" stiffness="0.2"/>
      </default>
      <default class="scapula_joint">
        <joint range="-50 40" pos="0 0 0" axis="0 1 0" stiffness="0.2"/>
      </default>
      <default class="shoulder_joint">
        <joint range="0 90" pos="0 0 0" axis="0 1 0" stiffness="0.2"/>
      </default>
      <default class="elbow_joint">
        <joint range="-90 0" pos="0 0 0" axis="0 1 0" stiffness="0.2"/>
      </default>
      <default class="wrist_joint">
        <joint range="-90 0" pos="0 0 0" axis="0 1 0" stiffness="0.2"/>
      </default>
      <default class="hip_joint">
        <joint range="-60 40" pos="0 0 0" axis="0 1 0" stiffness="0.2"/>
      </default>
      <default class="knee_joint">
        <joint range="0 85" pos="0 0 0" axis="0 1 0" stiffness="0.2"/>
      </default>
      <default class="ankle_joint">
        <joint range="-90 0" pos="0 0 0" axis="0 1 0" stiffness="0.2"/>
      </default>

      <!-- ext site = blue, flx site = green -->
      <default class="site_ext">
        <site rgba=".1 .1 1 1"/>
      </default>
      <default class="site_flx">
        <site rgba=".1 1 .1 1"/>
      </default>
      <default class="site_pass">
        <site rgba="1 1 .1 1"/>
      </default>

    </default>

    <!-- muscles -->
    <default class="muscle">
      <!-- Max human calf strength: 148.5 +/- 40.2 Nm (Trappe et al.,. 2001. Calf strength in humans) (STU)-->
      <!-- Thus, force = 148.5 Nm / 0.075 m = 1980 N = 19.8 scaled (STU)-->
      <!-- To keep gain values low, triple magnitude: force = 3 * 19.8 = 59.4 ~ 60 (STU)-->
      <muscle ctrllimited="true" ctrlrange="0 1" force="60"/>
    </default>
  </default>

