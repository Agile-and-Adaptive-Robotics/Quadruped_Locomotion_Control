        <site site="scapula_shoulder_R_flx_site_pass"/>
        <site site="humerus_shoulder_R_flx_site"/>
    </spatial>

    <spatial name="wrist_L_ext_tendon" rgba=".95 .3 .3 1">
        <site site="radius_wrist_L_ext_site"/>
        <site site="hand_wrist_L_ext_site"/>
    </spatial>
    <spatial name="wrist_L_flx_tendon" rgba=".95 .3 .3 1">
