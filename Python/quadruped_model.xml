    <muscle name="R_wrist_joint_ext_muscle" tendon="wrist_R_ext_tendon" class="muscle"/>
    <muscle name="R_wrist_joint_flx_muscle" tendon="wrist_R_flx_tendon" class="muscle"/>

    <muscle name="L_scapula_joint_ext_muscle" tendon="scapula_L_flx_tendon" class="muscle"/>
