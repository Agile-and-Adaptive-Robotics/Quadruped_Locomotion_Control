    <muscle name="L_wrist_joint_ext_muscle" tendon="wrist_L_ext_tendon" class="muscle"/>
    <muscle name="L_wrist_joint_flx_muscle" tendon="wrist_L_flx_tendon" class="muscle"/>

  </actuator>

  <!-- Close the loop for the extra bone in the humerus -->
  <equality>
      <connect body1="humerus_left_0" body2="radius_left" anchor="0 0 -0.1925"/>
      <connect body1="humerus_right_0" body2="radius_right" anchor="0 0 -0.1925"/>
  </equality>

</mujoco>
