    </spatial>
  </tendon>

  <actuator>

    <! Note that the ext and flx muscles are switched!!!! This is so that the neural network for the hindlimbs can be applied to the forelimbs >
    <muscle name="R_hip_joint_ext_muscle" tendon="hip_R_ext_tendon" class="muscle"/>
    <muscle name="R_hip_joint_flx_muscle" tendon="hip_R_flx_tendon" class="muscle"/>

    <muscle name="R_knee_joint_ext_muscle" tendon="knee_R_ext_tendon" class="muscle"/>
